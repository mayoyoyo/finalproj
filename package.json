{
  "name": "mp3js",
  "repository": "mp3js",
  "version": "0.1.0",
  "title": "mpThree.js",
  "description": "A music-based runner game",
  "keywords": [
    "Princeton",
    "graphics",
    "three.js",
    "COS 426",
    "ivy league",
    "music",
    "runner"
  ],
  "scripts": {
    "start": "webpack-dev-server --mode development --output-public-path '/'",
    "prebuild": "rimraf ./build && mkdir ./build",
    "build": "npm run prebuild && webpack --mode production --output-public-path '/'",
    "format": "prettier --write '*.js' 'src/**/*.js'",
    "deploy": "npm run prebuild && webpack --mode production && cp default.mp3 ./build/default.mp3 && cp Macintosh\\ Plus.mp3 ./build/Macintosh\\ Plus.mp3 && cp All\\ Star.mp3 ./build/All\\ Star.mp3 && gh-pages -d build"
  },
  "dependencies": {
    "bpm-detective": "^2.0.3",
    "dat.gui": "^0.7.7",
    "three": "^0.137.0",
    "three-obj-loader": "^1.1.3",
    "web-audio-api": "^0.2.2",
    "webpack-cli": "^3.3.11"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "babel-loader": "^8.1.0",
    "file-loader": "^6.0.0",
    "gh-pages": "^2.2.0",
    "html-webpack-plugin": "^4.2.1",
    "prettier": "^2.0.4",
    "raw-loader": "^4.0.1",
    "rimraf": "^3.0.2",
    "webpack": "^4.43.0",
    "webpack-dev-server": "^3.11.0"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
